{
  "name": "@dynamodb-reactive/client",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend proxy client and React hooks for reactive DynamoDB subscriptions",
  "keywords": [
    "dynamodb",
    "reactive",
    "client",
    "react",
    "hooks",
    "websocket",
    "realtime",
    "typescript"
  ],
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/client"
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "default": "./dist/react.js"
    }
  },
  "files": [
    "dist",
    "src",
    "!**/*.spec.*",
    "!**/*.test.*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "format": "eslint src --fix"
  },
  "dependencies": {
    "fast-json-patch": "^3.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
     "eslint": "8.57.1"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
