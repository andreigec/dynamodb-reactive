{
  "name": "dynamodb-reactive",
  "version": "0.1.0",
  "private": true,
  "description": "A Serverless, Reactive tRPC replacement for AWS",
  "keywords": [
    "dynamodb",
    "reactive",
    "realtime",
    "websocket",
    "aws",
    "serverless",
    "trpc",
    "typescript"
  ],
  "license": "MIT",
  "author": "andreigec@gmail.com",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "pnpm -r build",
    "clean": "pnpm -r clean && pnpm -r exec rimraf dist",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format",
    "typecheck": "pnpm -r typecheck",
    "prepublishOnly": "pnpm build",
    "publish": "pnpm --filter dynamodb-harness publish --access public",
    "version:patch": "pnpm -r exec npm version patch",
    "version:minor": "pnpm -r exec npm version minor",
    "version:major": "pnpm -r exec npm version major"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint-config-e7npm": "^0.1.31",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}
